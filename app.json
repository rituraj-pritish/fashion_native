{
  "expo": {
    "name": "fashion_native",
    "slug": "fashion_native",
    "platforms": ["ios", "android", "web"],
    "packagerOpts": {
      "config": "metro.config.js",
      "assetExts": ["ttf"],
      "sourceExts": ["js", "jsx", "svg", "svgx"]
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#dadada"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
